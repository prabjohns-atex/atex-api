{
       	"budget-view": [{
       		"id": "default-budget-view",
       		"description": "Budget View - Default Configuration",
       		"config": {
       			"budget-view": {
       				"componentId": "desk-budget-view",
       				"html": "<div class='table-responsive' ng-if='showList && entries.length>0' class='jobsList' ng-init='watchEntries()' style='overflow: auto;height: 800px;'>   <ul class='list-group' ng-repeat='(key, entry) in entries'       ng-init='poll(entry)'       name={{entry._data.aspects.contentData.data.name}}       type='{{::entry._data.aspects.contentData.data.inputTemplate}}' ng-class='appendEmpty(entry)'>      <div ng-if='isNewSubHead(entry)' class='subhead'>{{entry._data.aspects.contentData.data.deskName}}</div>      <li ng-if='(!deadlineSortSelected() && !enterpriseSortSelected() && !tvSortSelected() && !isPendingJob(entry))          || (deadlineSortSelected() && hasDeadlineInFuture(entry) && !isPendingJob(entry))          || (enterpriseSortSelected() && isEnterpriseBudget(entry) && !isPendingJob(entry))  || (tvSortSelected() && entry.hasVideoTask && !isPendingJob(entry)) '          class='list-group-item d-flex top-item' ng-click='clickOnBudgetItem(entry,$event);'          contentId='{{::entry.id}}' ng-class='isSelected(entry);'>         <div>            <a ng-click='edit([entry]);'><small class='slug'>{{entry._data.aspects.contentData.data.name || 'No name'}}</small></a>             <small class='authorName' ng-if='!segmentSortSelected() && hasAuthor(entry, 1)'><span class='break'> | </span>{{::entry.articleData.reporter1_simple}}</small>             <small class='authorName' ng-if='!segmentSortSelected() && hasAuthor(entry, 2)'>/ {{::entry.articleData.reporter2_simple}}</small>             <small class='mmjName' ng-if='hasMMJ(entry)'><span class='break'> | </span>MMJ: {{::entry.videoData.mmjName}}</small>             <small class='wordCount' ng-if='!segmentSortSelected() && hasLength(entry)'><span class='break'>| </span>{{::entry.articleData.displayLength}}</small>             <i class='fa fa-newspaper-o publication-icon' ng-mouseover='hoverIn(entry)' ng-mouseleave='hoverOut(entry)' ng-if='isPublEventPresent(entry)' style='margin-left:5px; margin-right:5px; color: blue;'></i>                                                                                                 <div ng-if='isPublEventPresent(entry)' style='position: absolute;z-index: 100;display:inline; background-color: white;border: 1px solid black;margin-top: 20px;max-width:450px;' ng-show='entry.hoverEdit'>{{::getPopOverText(entry)}}</div>            <div class='status printRdy' ng-if='isPrintReady(entry)'>COPIED TO PAGE</div>            <div ng-class='setPublishedLabel(entry);' ng-if='!segmentSortSelected() && isPublished(entry)'>PUBLISHED</div>            <div class='status attention' ng-if='isSpecialAttention(entry)'>SPECIAL ATTENTION</div>            <div class='status attention' ng-if='isTvJob(entry)'>TV</div>            <br>                                                                                                 <div class='deadline' ng-if='!isTvJob(entry) && (hasArticleOnTime(entry) || hasJobDeadline(entry))'>{{::entry._data.aspects.contentData.data.digitalDeadlineLabel}}</div>            <div class='stars' ng-if='!segmentSortSelected() && hasRelevance(entry)'>                <i ng-class='setStar($index, entry);' ng-repeat='i in [1, 2, 3, 4, 5, 6]'></i>             </div>            <br>                         <!-- restored articleshortlogic-->                                                                                    <a ng-if='!segmentSortSelected() && hasArticleShortTitle(entry)' ng-click='edit([entry]);'>               <h5 class='mb-1'>{{entry.articleData.displayShortHeadline}}</h5>            </a>            <a ng-if='!segmentSortSelected() && !hasArticleShortTitle(entry)' ng-click='edit([entry]);'>               <h5 class='mb-1'>{{entry._data.aspects.contentData.data.budgetHeadline}}</h5>            </a>            <a ng-if='segmentSortSelected() && hasArticleShortTitle(entry)' ng-click='edit([entry]);'>               <h5 class='mb-1'>{{entry._data.aspects.contentData.data.tvHeadline || entry._data.aspects.contentData.data.displayShortHeadline || entry._data.aspects.contentData.data.budgetHeadline}}</h5>            </a>            <a ng-if='segmentSortSelected() && !hasArticleShortTitle(entry)' ng-click='edit([entry]);'>               <h5 class='mb-1'>{{entry._data.aspects.contentData.data.tvHeadline || entry._data.aspects.contentData.data.budgetHeadline}}</h5>            </a>            <div ng-if='hasNote(entry)'><b>{{entry._data.aspects['atex.WFContentStatus'].data.comment}}</b></div>            <p ng-if='!segmentSortSelected() && hasArticleBrief(entry)'>               {{entry.articleData.displayBrief}}            </p>            <p ng-if='!segmentSortSelected() && !hasArticleBrief(entry)'>               {{entry._data.aspects.contentData.data.description}}            </p>            <p ng-if='segmentSortSelected() && hasArticleBrief(entry)'>               {{entry._data.aspects.contentData.data.tvDescription || entry._data.aspects.contentData.data.displayBrief || entry._data.aspects.contentData.data.description}}            </p>            <p ng-if='segmentSortSelected() && !hasArticleBrief(entry)'>               {{entry._data.aspects.contentData.data.tvDescription || entry._data.aspects.contentData.data.description}}            </p>            <!-- restored articleshortlogic-->                                                                                    <div class='content-status' ng-if='!segmentSortSelected()'>                <span ng-if='hasNewsLabel(entry)'>                <b ng-if='hasNewsLabel(entry)'>News Label: </b>{{entry.articleData.newsLabel}}                <b ng-if='hasNewsLabel(entry)' class='divider'> | </b>                </span>                <span                   ng-if='isCollectionComplete(task) || isVideoComplete(task) || isWeblinkComplete(task) || isSocialMediaComplete(task) || isAudioComplete(task)'                   ng-repeat='task in entry._data.aspects.contentData.data.objects'>                <b ng-if='isCollectionComplete(task)'>{{getLabel(task._data.aspects.contentData.data);}}: </b>{{task._data.aspects.contentData.data.collectionStatus}}                <b ng-if='isVideoComplete(task)'>{{getLabel(task._data.aspects.contentData.data);}} </b>{{task._data.aspects.contentData.data.videoStatus}}                <b ng-if='isWeblinkComplete(task)'>{{getLabel(task._data.aspects.contentData.data);}}: </b>{{task._data.aspects.contentData.data.webLinkStatus}}                <b ng-if='isSocialMediaComplete(task)'>{{getLabel(task._data.aspects.contentData.data);}}: </b>{{task._data.aspects.contentData.data.socialMediaStatus}}                <b ng-if='isAudioComplete(task)'>{{getLabel(task._data.aspects.contentData.data);}}: </b>{{task._data.aspects.contentData.data.audioStatus}}                <b ng-if='isCollectionComplete(task) || isVideoComplete(task) || isWeblinkComplete(task) || isSocialMediaComplete(task) || isAudioComplete(task)' class='divider'> | </b>                </span>             </div>            <div class='content-status' ng-if='segmentSortSelected()  && entry.videoWfStatus && entry.videoWebStatus'>    <strong>Video Status: </strong> {{entry.videoWfStatus}}<strong> | Video Web Status: </strong> {{entry.videoWebStatus}}   </div>         </div>         <span ng-if='hasRelated(entry)'>          <i ng-if='!entry.show' class='fa fa-chevron-down cursor-pointer pull-right' ng-click='showRelated(entry);$event.stopPropagation();' title='{{'_TASK_LIST_LABEL_' | translate}}'></i>          <i ng-if='entry.show' class='fa fa-chevron-up cursor-pointer pull-right' ng-click='hideRelated(entry);$event.stopPropagation();' title='{{'_TASK_LIST_LABEL_' | translate}}'></i>          </span>                                                 </li>      <li ng-if='entry.show' class='status-divider' ng-click='$event.stopPropagation();'>STATUS</li>      <li ng-if='entry.show' ng-repeat='task in entry._data.aspects.contentData.data.objects' class='list-group-item d-flex budget-item' ng-click='clickOnBudgetItem(task,$event); isLocked(task);' contentId='{{::task.id}}' ng-class='isSelected(task);'>         <div draggable='true' name='{{::task.data.aspects.contentData.data.name}}' type='{{::task.data.aspects.contentData.data.inputTemplate}}' contentId='{{::task.id}}' contentType='{{::task.data.aspects.contentData.data.objectType}}'>            <div class='title'>               <small ng-if='isArticle(task)'>{{task._data.aspects.contentData.data.storyType}}</small>                <small ng-if='isCollection(task)'>{{task._data.aspects.contentData.data.collectionType}}</small>                <small ng-if='isVideo(task)'>{{task._data.aspects.contentData.data.videoType}}</small>                <small ng-if='isWebLink(task)'>{{task._data.aspects.contentData.data.linkType}}</small>                <small ng-if='isUserContent(task)'>{{task._data.aspects.contentData.data.itemType}}</small>                <small ng-if='!isArticle(task) && !isCollection(task) && !isVideo(task) && !isWebLink(task) && !isUserContent(task)'>{{task._data.aspects.contentData.data.objectType}}</small>                <br>                                                                                                                         <a ng-if='!isVideo(task)' ng-controller='DamClipboardController' ng-click='open([task]); $event.stopPropagation();'>                  <h6 class='mb-1'>{{task._data.aspects.contentData.data.displayTitle}}</h6>               </a>               <a ng-if='isVideo(task)' ng-click='edit([task]);'>                  <h6 class='mb-1'>{{task._data.aspects.contentData.data.displayTitle}}</h6>               </a>               <br>                <span ng-if='task._data.aspects.contentData.data.lastExportTime' >{{'_LAST-POSTED_' | translate}}&nbsp;</span>                <i ng-if='task._data.aspects.contentData.data.lastExportTime'>{{task._data.aspects.contentData.data.lastExportTime | date: 'h:mm a EEE. MMM. d'}}</i>                                                                                                 </div>            <div class='current'>               <i ng-if='!task.versionInfo && isLocked(task) && !!!checkingLock' ng-click='checkLock(task);$event.stopPropagation();' class='icon-right fa fa-lock fa-lg dam-locked-item-icon'                ng-class='{\"dam-locked-byme-item-icon\": task._data.lockData.byMe}'                title='{{::task._data.lockData.getUserName()}} {{task._data.lockData.timestamp | date:'MM/dd/yyyy @ h:mma'}} in {{::task._data.lockData.app}}'></i>                <i ng-if='checkingLock' ng-click='checkLock(task);$event.stopPropagation();' class='icon-right fa fa-spinner fa-spin fa-lg dam-locked-item-icon active'></i>                <span ng-repeat='(key,attr) in getAttributesList(task._data.aspects[\"atex.WFContentStatus\"].data.status.attributes); track by key'>                <i class='fa fa-globe' ng-if='attr.icon'><i class='{{attr.icon}} active'></i></i>                </span>                                                                                                                         <div class='status' style='{{task.labelColor}};width: 85px;font-size: x-small;padding: 1px;'>Web Status:<br>{{getStatusName(task._data.aspects['atex.WebContentStatus'].data.status.statusID)}}</div>               <div id='editable-status-widget'>                  <ui-editable-status path='wfStatuspath' status='getStatus(task)' entry='task' list='statusList'></ui-editable-status>               </div>            </div>         </div>      </li>   </ul></div>",
       				"budget-sort": [
                      { "label": "Desk", "labelUI": "Print Pubdate", "value": "desk_atex_desk_i,relevance_atex_desk_s" , "sort": "asc,desc" },
                      { "label": "Newsday TV", "labelUI": "All Videos", "value": "desk_atex_desk_i,digitalpublishingtime_atex_desk_dt", "sort": "asc,asc"},
                      { "label": "Segments", "labelUI": "SMain Event", "value": "tvpublishingtime_atex_desk_dt,relevance_atex_desk_s" , "sort": "asc,asc"},
                      { "label": "Planning", "labelUI": "Planning", "value": "desk_atex_desk_i,digitalpublishingtime_atex_desk_dt", "sort": "asc,asc" },
                      { "label": "Rating", "labelUI": "Rating", "value": "relevance_atex_desk_s" , "sort": "desc"},
                      { "label": "Digital Deadline", "labelUI": "Digital Deadline", "value": "digitalpublishingtime_atex_desk_dt" , "sort": "asc"}
                    ]
       			}
       		}
       	}]
       }
